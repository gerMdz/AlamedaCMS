<section class="{{ section. }}" id="{{ section.identificador|default('section') }}">
    {% if section.contenido %}
        <div class="container text-center">
            <h4 class="text-alameda">
                <b class="aire-2 fuente-arial-nova">
                    {{ section.contenido | replace({'<p>':'', '</p>':'<br/>'}) | raw }}
                </b>
            </h4>
            <hr/>
        </div>
    {% endif %}
    <div class="container mt-5">
        <div class="row">
            <div class="col2"></div>
            {% for entrada in entradas %}
                {% include 'modelEntrada/'~entrada.modelTemplate~'.html.twig' with { 'section':entrada } %}
            {% endfor %}
        </div>
    </div>
</section>