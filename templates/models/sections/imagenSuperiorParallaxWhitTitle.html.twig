<style>
    .sobreImage{
        margin-top: 0 !important;
    }
    .sinZ-index{
        z-index: 0 !important;
    }
    .imagen-responsive{
        max-width: 100%;
        height: auto;
    }
</style>
<div class="mt-sm-5 pt-sm-5 mt-md-1 pt-md-1" style="height: 1em" >

</div>
    <section class="bg-gris-medio text-white my-5">

        <!-- Promo Content -->
        <div class="container-fluid sinZ-index px-0">
            <div class="row justify-content-center">
                <div class="col-12 m-0 text-center">
                        <img src="{{section.imagePath|imagine_filter('squared_thumbnail_large_notas')}}" class="imagen-responsive" alt="Responsive image">
                </div>
            </div>
        </div>
        <!-- End Promo Content -->
    </section>

<main role="main" class="card {{ section.cssClass | default(' text-dark') }}" id="{{ section.identificador }}">
    <div class="col-sm-12 card-header ml-0">
        <h2 class="">{{ section.title | replace({'<p>':'', '</p>':'<br/>'}) | raw }} </h2>
        <small>{{ section.contenido | replace({'<p>':'', '</p>':'<br/>'}) | raw }} </small>
    </div>
</main>
