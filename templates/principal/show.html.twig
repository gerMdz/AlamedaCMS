{% extends 'base.html.twig' %}

{% block title %}Principal{% endblock %}

{% block body %}
    <h1>Principal</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ principal.id }}</td>
            </tr>
            <tr>
                <th>Titulo</th>
                <td>{{ principal.titulo }}</td>
            </tr>
            <tr>
                <th>Contenido</th>
                <td>{{ principal.contenido }}</td>
            </tr>
            <tr>
                <th>LinkRoute</th>
                <td>{{ principal.linkRoute }}</td>
            </tr>
            <tr>
                <th>ImageFilename</th>
                <td>{{ principal.imageFilename }}</td>
            </tr>
            <tr>
                <th>Likes</th>
                <td>{{ principal.likes }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ principal.createdAt ? principal.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ principal.updatedAt ? principal.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('principal_index') }}">back to list</a>

    <a href="{{ path('principal_edit', {'id': principal.id}) }}">edit</a>

    {{ include('principal/_delete_form.html.twig') }}
{% endblock %}
