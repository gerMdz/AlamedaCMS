    <div class="col-md-12">
        <div class="card">
            <div class="card-header card-header-primary">
                <h4 class="card-title ">Principal</h4>
                <p class="card-category"> Listado de p치ginas principales</p>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table">
                        <thead class=" text-primary">

                        <tr>
                            <th>Id</th>
                            <th>Titulo</th>
                            <th>Contenido</th>
                            <th>LinkRoute</th>
                            <th>ImageFilename</th>
                            <th>Likes</th>
                            <th>Creada</th>
                            <th>Actualizada</th>
                            <th>actions</th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for principal in principals %}
                            <tr>
                                <td>{{ principal.id }}</td>
                                <td>{{ principal.titulo }}</td>
                                <td>{{ principal.contenido | u.truncate(15, '...') }}</td>
                                <td>{{ principal.linkRoute }}</td>
                                <td>{{ principal.imageFilename }}</td>
                                <td>{{ principal.likes }}</td>
                                <td>{{ principal.createdAt ? principal.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                                <td>{{ principal.updatedAt ? principal.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                                <td>
                                    <a href="{{ path('principal_ver', {'linkRoute': principal.linkRoute}) }}">
                                        <i class="fas fa-eye"></i>
                                        Ver</a>
                                    <a href="{{ path('principal_edit', {'id': principal.id}) }}">
                                        <i class="fas fa-edit"></i>
                                        Editar</a>
                                </td>
                            </tr>
                        {% else %}
                            <tr>
                                <td colspan="9">No se encontr칩 ninguna p치gina prinpipal</td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="card-footer">

                <a href="{{ path('principal_new') }}" class="btn btn--pill btn-primary">
                    <i class="fas fa-plus-circle"></i>
                    Crear nueva p치gina prinpipal</a>
            </div>
        </div>
    </div>
