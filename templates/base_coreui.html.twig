<!DOCTYPE html>
<html lang="es">
<head>
    {% if google_tag_enable == 'true' %}
        {% include('inicio/_etiqueta_global_google.html.twig') %}
    {% endif %}
    {% set base = base_base() %}
    {% set version = 'now' | date('YmdH') %}
    <base href="./">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <meta name="description" content="{{ base.siteName }},  {{ base_metaDescripcion() }}">
    <meta name="author" content="{{ base.metaAutor }}">
    <meta name="keywords" content="{{ base.siteName }},{{ base.lema }},{{ base_metaDescripcion() }}">
    <title>
        {% block title %}{{ base.siteName }} Bienvenido! {% endblock %}
    </title>
    <link rel="shortcut icon" href="{{ asset('/images/institucional/indice.png') }}" type="image/x-icon">


    <link rel="manifest" href="assets/favicon/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="assets/favicon/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">

    {% block icons %}
        <link href="{{ asset('icons/foundation-icons/foundation-icons.css') }}" rel="stylesheet">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css" rel="stylesheet">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    {% endblock %}

    {% block fonts %}
        <link href="{{ asset('fonts/fonts.css') }}?v={{ version }}" rel="stylesheet">
        <link href="{{ asset('fonts/fontAlameda.css') }}?v={{ version }}" rel="stylesheet">
    {% endblock %}

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('fonts/awesome/web-fonts-with-css/css/all.min.css') }}">
        <link rel="stylesheet" href="{{ asset('fonts/awesome/web-fonts-with-css/css/v4-shims.min.css') }}">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons+Round" rel="stylesheet">
        <link rel="stylesheet" href="{{ asset('kit/coreui/vendors/simplebar/css/simplebar.min.css') }}">
        <link rel="stylesheet" href="{{ asset('kit/coreui/css/vendors/simplebar.css') }}">
        <link href="{{ asset('kit/coreui/css/style.min.css') }}" rel="stylesheet">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/prismjs@1.23.0/themes/prism.css">
        <link href="{{ asset('kit/coreui/css/examples.css') }}" rel="stylesheet">
        {{ encore_entry_link_tags('select2-parent') }}

    {% endblock %}

    {#    <link href="vendors/@coreui/chartjs/css/coreui-chartjs.css" rel="stylesheet"> #}
    {% if(facebook_tag_enable == 'true') %}
        {% include('inicio/_facebook_pixel.html.twig') %}
    {% endif %}
</head>
<body>
<div class="body flex-grow-1 px-3">
    <div class="container-lg">
        <div class="row">
            {% block content %}
                <!-- your content here -->
            {% endblock %}
        </div>
    </div>
</div>
{% block javascripts %}
    <script src="{{ asset('material-dashboard/assets/js/core/jquery.min.js') }}" type="text/javascript"></script>
    <!-- CoreUI and necessary plugins-->
    <script src="{{ asset('kit/coreui/vendors/@coreui/coreui/js/coreui.bundle.min.js') }}"></script>
    <script src="{{ asset('kit/coreui/vendors/simplebar/js/simplebar.min.js') }}"></script>
    <!-- We use those scripts to show code examples, you should remove them in your application.-->
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.24.1/prism.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.24.1/plugins/autoloader/prism-autoloader.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.24.1/plugins/unescaped-markup/prism-unescaped-markup.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.24.1/plugins/normalize-whitespace/prism-normalize-whitespace.js"></script>
    <!-- Plugins and scripts required by this view-->
    {#    <script src="{{ asset('kit/coreui/vendors/chart.js/js/chart.min.js') }}"></script> #}
    {#    <script src="vendors/@coreui/chartjs/js/coreui-chartjs.js"></script> #}
    <script src="{{ asset('kit/coreui/vendors/@coreui/utils/js/coreui-utils.js') }}"></script>
    {#    <script src="{{ asset('kit/coreui/js/main.js') }}"></script> #}

    {{ encore_entry_script_tags('select2-parent') }}
    {{ encore_entry_script_tags('section_select') }}

{% endblock %}
</body>
</html>