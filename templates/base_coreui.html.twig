    <link rel="shortcut icon" href="{{ asset('/images/institucional/indice.png') }}" type="image/x-icon">


    <link rel="manifest" href="{{ asset('images/favicon/manifest.json') }}">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="assets/favicon/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">

    {% block icons %}
        <link href="{{ asset('icons/foundation-icons/foundation-icons.css') }}" rel="stylesheet">
        <link href="{{ asset('fonts/awesome/web-fonts-with-css/css/all.min.css') }}" rel="stylesheet">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    {% endblock %}

    {% block fonts %}
        <link href="{{ asset('fonts/fonts.css') }}?v={{ version }}" rel="stylesheet">
        <link href="{{ asset('fonts/fontAlameda.css') }}?v={{ version }}" rel="stylesheet">
    {% endblock %}

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('fonts/awesome/web-fonts-with-css/css/all.min.css') }}">
        <link rel="stylesheet" href="{{ asset('fonts/awesome/web-fonts-with-css/css/v4-shims.min.css') }}">
