{
    "type": "project",
    "license": "MIT",
    "name": "germdz/alamedacms",
    "description": "CMS Sencillo con reservas por email, y manejo de usuarios, bootstrap en CSS",
    "require": {
        "php": "^8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^3.2.1",
        "doctrine/dbal": "^3.2",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.10",
        "endroid/qr-code-bundle": "4.3.*",
        "erusev/parsedown": "^1.6",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "gedmo/doctrine-extensions": "^v3.4.0",
        "knplabs/knp-markdown-bundle": "^1.8",
        "knplabs/knp-paginator-bundle": "^5.2",
        "knplabs/knp-time-bundle": "^1.11",
        "laminas/laminas-code": "^4.7",
        "laminas/laminas-eventmanager": "^3.2",
        "liip/imagine-bundle": "^2.7.4",
        "oneup/flysystem-bundle": "^4.11.2",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpoffice/phpspreadsheet": "^1.15",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^6.1",
        "stof/doctrine-extensions-bundle": "^1.10.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/expression-language": "6.0.*",
        "symfony/flex": "^1.17.1",
        "symfony/form": "^v5.4.0",
        "symfony/framework-bundle": "^v5.4.0",
        "symfony/http-client": "5.4.*",
        "symfony/intl": "6.0.*",
        "symfony/mailer": "6.0.*",
        "symfony/polyfill-intl-messageformatter": "^1.12",
        "symfony/polyfill-php72": "^1.15",
        "symfony/property-access": "^v5.4.0",
        "symfony/property-info": "5.4.*",
        "symfony/runtime": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/string": "6.0.*",
        "symfony/translation": "6.0.*",
        "symfony/twig-bridge": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/ux-turbo": "^2.1",
        "symfony/validator": "6.0.*",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/yaml": "6.0.*",
        "symfonycasts/reset-password-bundle": "^1.17",
        "tgalopin/html-sanitizer-bundle": "^1.4",
        "twig/cssinliner-extra": "^3.7",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^3.1",
        "twig/intl-extra": "^3.1",
        "twig/markdown-extra": "^3.3",
        "twig/string-extra": "^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "*",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.19.0",
        "symfony/browser-kit": "6.0.*",
        "symfony/css-selector": "6.0.*",
        "symfony/debug-bundle": "6.0.*",
        "symfony/maker-bundle": "^1.19",
        "symfony/monolog-bundle": "^3.1",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "6.0.*",
        "symfony/var-dumper": "*",
        "symfony/web-profiler-bundle": "6.0.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "endroid/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.0.*"
        }
    }
}
